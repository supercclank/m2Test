//Default Taks set to jar
defaultTasks "compileJava"

//Apply Java Plugin
apply plugin: "java"

//Set up some variables (properties)
sourceCompatibility = 1.7
version = '1.0'

dependencies {
    compile fileTree(dir: 'lib', include: 'resources.jar')
}

jar {
    mainfest {
        attributes 'Implementation-Title': 'Gradle Quickstart', 'Implementation-Version': version , "Main-Class": "AntLabMain"
    }
}

repositories {
    mavenCentral()
}